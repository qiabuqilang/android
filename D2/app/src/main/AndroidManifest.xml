<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.d2">
    <!-- 拨打电话 -->
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <!-- 发送短信 -->
    <uses-permission android:name="android.permission.SEND_SMS" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RelativeXmlActivity"/>

        <activity android:name=".RelativeCodeActivity"/>

        <activity android:name=".FrameActivity"/>

        <activity android:name=".CheckActivity"/>

        <activity android:name=".SwitchDefaultActivity"/>

        <activity android:name=".SwitchIosActivity"/>

        <activity android:name=".RadioHorizontalActivity"/>

        <activity android:name=".RadioVerticalActivity"/>

        <activity android:name=".SpinnerDropdownActivity"/>

        <activity android:name=".SpinnnerDialogActivity"/>

        <activity android:name=".SpinnerIconActivity"/>

        <activity android:name=".EditSimpleActivity"/>

        <activity android:name=".EditCursorActivity"/>

        <activity android:name=".EditBorderActivity"/>

        <activity android:name=".EditHideActivity"/>

        <activity android:name=".EditJumpActivity"/>

        <activity android:name=".EditAutoActivity"/>

        <activity android:name=".ActNextActivity"/>

        <activity android:name=".ActJumpActivity"/>

        <activity android:name=".ActRotateActivity"/>

        <activity android:name=".ActHomeActivity"/>

        <activity android:name=".ActUriActivity"/>

        <activity android:name=".ActRequestActivity"/>

        <activity android:name=".ActResponseActivity"/>
    </application>


</manifest>